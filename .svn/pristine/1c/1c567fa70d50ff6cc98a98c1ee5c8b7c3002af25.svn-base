<div class="content">
	<div class="content_area">
		<div class="content_area_title">
			<h2>发布新楼盘</h2>
		</div>
		<div class="content_main">
			<div class="inputArea">
				<h3>创建新活动</h3>
				<form>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 选择类型：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text">
								</select>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>选择地区：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text shortSelect"></select>
								<span>省</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>市</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>区</span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 选择楼盘：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text"></select>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 活动名称：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text hasUnitInput" id="username" name="username" placeholder=""><span class="hasUnitInput_unit"></span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-large" style="border: dashed;height: 200px;">
							<label for="username"><span class="requireMark">*</span> 选择房源：<span class="justify"></span></label>
							<div class="form-field">
								<input type="button" value="添加已有房源" />
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-broad">
							<label for="username"><span class="requireMark">*</span> 购房特惠：<span class="justify"></span></label>
							<div class="form-field">
								<textarea class="form-text" id="username" name="username" placeholder=""></textarea>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-large">
							<label for="username"><span class="requireMark">*</span> 开始时间：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text shortSelect"></select>
								<span>年</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>月</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>日</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>时</span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-large">
							<label for="username"><span class="requireMark">*</span> 结束时间：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text shortSelect"></select>
								<span>年</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>月</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>日</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>时</span>
							</div>
						</div>
					</div>
				</form>
			
		</div>
