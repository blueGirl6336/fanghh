package hitwh.fanghh.bean;

public class PerferentialRuleBean {
	private String perferentialRuleType; //活动规则类型（满减抵等等）
	private String perferentialRuleTitle;//活动规则标题
	private String perferentialDescription;//活动规则描述
	private Integer ruleId;//主键
	private Integer activityId;//activity表外键
	
	public String getPerferentialRuleType()
	{
		return perferentialRuleType;
	}
	public void setPerferentialRuleType(String perferentialRuleType)
	{
		this.perferentialRuleType = perferentialRuleType;
	}
	


	public String getPerferentialDescription()
	{
		return perferentialDescription;
	}
	public void setPerferentialDescription(String perferentialDescription)
	{
		this.perferentialDescription = perferentialDescription;
	}
	
	public String getPerferentialRuleTitle()
	{
		return perferentialRuleTitle;
	}
	public void setPerferentialRuleTitle(String perferentialRuleTitle)
	{
		this.perferentialRuleTitle = perferentialRuleTitle;
	}
	
	public Integer getRuleId()
	{
		return ruleId;
	}
	public void setRuleId(Integer ruleId)
	{
		this.ruleId = ruleId;
	}
	
	public int getActivityId()
	{
		return activityId;
	}
	public void setActivityId(Integer activityId)
	{
		this.activityId = activityId;
	}
	
}