<div class="content">
	<div class="content_area">
		<div class="content_area_title">
			<h2>发布新楼盘</h2>
		</div>
		<div class="content_main">
			<div class="hint">
				<p class="hint_text"></p>
			</div>
			<div class="inputArea">
				<h3>楼盘录入</h3>
				<form>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 楼盘名称：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>所在区域：<span class="justify"></span></label>
							<div class="form-field">
								<select name="" id="" class="form-text shortSelect"></select>
								<span>省</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>市</span>
								<select name="" id="" class="form-text shortSelect"></select>
								<span>区</span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 用户名：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-large">
							<label><span class="requireMark">*</span>房屋类型：</label>
							<div class="form-field">
								<label>
									<input type="checkbox" checked="checked" value="" class="form-checkbox" name="">
									<span class="choiceLabel">普通住宅</span>
								</label>
								<label>
									<input type="checkbox" value="" class="form-checkbox" name="">	
									<span class="choiceLabel">公寓</span>
								</label>
								<label>
									<input type="checkbox" value="" class="form-checkbox" name="">
									<span class="choiceLabel">花园洋房</span>
								</label>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 用户名：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>开盘时间：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>入住时间：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>建筑面积：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>总户数：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 用户名：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text hasUnitInput" id="username" name="username" placeholder=""><span class="hasUnitInput_unit">元</span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span> 用户名：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>产权年限：<span class="justify"></span></label>
							<div class="form-field">
								<input type="text" class="form-text" id="username" name="username" placeholder="">
							</div>
						</div>
						<div class="form-item form-item-middle">
							<label for="username"><span class="requireMark">*</span>装修情况：<span class="justify"></span></label>
							<div class="form-field">
								<select type="text" class="form-text" id="username" name="username" placeholder=""></select>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-broad">
							<label for="username"><span class="requireMark">*</span> 用户名：<span class="justify"></span></label>
							<div class="form-field">
								<textarea class="form-text" id="username" name="username" placeholder=""></textarea>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="form-item form-item-large">
							<label>楼盘类型：<span class="justify"></span></label>
							<div class="form-field">
								<label>
									<input type="radio" checked="checked" value="man" class="form-radio" name="bdType"><span class="choiceLabel">在售</span>
								</label>
								<label>
									<input type="radio" value="woman" class="form-radio" name="bdType"><span class="choiceLabel">待售</span>
								</label>
								<label>
									<input type="radio" value="woman" class="form-radio" name="bdType"><span class="choiceLabel">售罄</span>
								</label>
							</div>
						</div>
					</div>
					
				</form>
