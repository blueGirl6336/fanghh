@import "list.scss";

#buildingManagement
{
	height:45px;
	border-right:1px solid rgb(229,229,229);
	border-left:0px;
	border-bottom:0px;
	border-top:7px solid rgb(253,103,32);
	z-index:2;
	background-color:#fff;
	font-weight:bold;
}
.content
{
	padding:30px 35px 0px 20px;
}
.content-div
{
	display: -webkit-flex; //Safari
  	display: flex;
  	flex-direction:column;
}
.content_area_head
{
	width:100%;
	height:60px;
	background-color:#fff;
	border:1px solid;
	border-color:rgb(229,229,229);
}
.table_area
{
	margin:10px 0px 35px 0px;
	background-color:#fff;
	border:1px solid;
	border-color:rgb(229,229,229);
}

.table_head
{
	height:50px;
	border:1px solid;
	border-color:rgb(229,229,229);
}

.table_content
{
	background-color:#fff;
}
.building_table
{
	position:relative;
	top:20px;
	border:0px;
	border-collapse:collapse;
	tr 
	{
		display: -webkit-flex; //Safari
  		display: flex;
		flex-direction:row;
		height:45px;
		width:100%;
		margin:0px;
		border:1px solid;
		border-color:#ebebeb;
		// padding-right:40px;
		td
		{
			display: -webkit-flex; //Safari
  			display: flex;
			height:45px;
			// flex-direction:row;
			justify-content: center;
			align-items:center;
			font-size: 12px;
			color:#666;
		}
	}
	.line0
	{
		background-color:rgb(244,244,244);
		.sort-up
		{
			position: relative;
			left:8px;
		}
		.sort-down
		{
			position:relative;
			left:15px;
		}
	}
	.line1
	{
		background-color:#fff;
		.operation1,.operation2,.recommend,.manage
		{
			color:rgb(0,102,204);
		}
		.manage-operation
		{
			color: rgb(253,103,32);
		}
	}
	.line2
	{
		background-color:rgb(239,243,251);
		.operation1,.operation2,.recommend,.manage
		{
			color:rgb(0,102,204);
		}
		.manage-operation
		{
			color: rgb(253,103,32);
		}
	}
	.checkbox,.team-buy
	{
		width:80px;
	}
	.building-name
	{
		width:140px;
	}
	.state,.operation
	{
		width:150px;
	}
	.operation1,.operation2
	{
		width:75px;
	}
	.tele,.modify-date
	{
		width:170px;
	}
	.manage-operation
	{
		width:120px;
	}
	.recommend
	{
		width:120px;
	}
	.manage
	{
		width:70px;
	}
	.online
	{
		width:75px;
	}
	.recommend-or-not
	{
		width:110px;
	}

	.table-bottom
	{
		background-color:#fff;
	}
	.checkbox-bottom
	{
		width:106px;
	}
	.up-select,.down-select
	{
		width:80px;
		text-decoration: underline;
	}
}

.page-num
{
	display: -webkit-flex; //Safari
  	display: flex;
	height:150px;
	flex-wrap:nowrap;
	justify-content:center;
	align-items:center;
	font-size:14px;
	color:#999;
	.first
	{
		display: -webkit-flex; //Safari
  		display: flex;
		height:30px;
		width:50px;
		border:1px solid #efefef;
		justify-content:center;
		align-items:center;
		margin-left:5px;
		margin-right:5px;
	}
	.before
	{
		@extend.first;
		width:60px;
	}
	.page-1
	{
		@extend .first;
		height:30px;
		width:30px;
	}
	.page-2
	{
		@extend .page-1;
	}
	.page-3
	{
		@extend .page-1;
	}
	.page-4
	{
		@extend .page-1;
	}
	.page-5
	{
		@extend .page-1;
	}
	.next
	{
		@extend .before;
	}
	.last
	{
		@extend .first;
	}
	.first:hover,.page-1:hover,.page-2:hover,.page-3:hover,.page-4:hover,.page-5:hover,.next:hover,.last:hover
	{
		cursor: pointer;
	}
}
